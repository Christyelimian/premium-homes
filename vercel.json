{
  "version": 2,
  "name": "premium-homes", 
  "buildCommand": "cd national-blueprint-main && npm run build",
  "outputDirectory": "national-blueprint-main/dist",
  "installCommand": "cd national-blueprint-main && npm install",
  "framework": "vite",
  

  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/national-blueprint-main/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]

  ],
  "cleanUrls": true,
  "trailingSlash": false
}