{
  "version": 2,
  "name": "premium-homes", 
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rootDirectory": "national-blueprint-main",
  "rewrites": [
    {
      "source": "/ng/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/sl/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bw/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bf/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!assets/|api/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}